# Your snippets
#
# Atom snippets allow you to enter a simple prefix in the editor and hit tab to
# expand the prefix into a larger code block with templated values.
#
# You can create a new snippet in this file by typing "snip" and then hitting
# tab.
#
# An example CoffeeScript snippet to expand log to console.log:
#
# '.source.coffee':
#   'Console log':
#     'prefix': 'log'
#     'body': 'console.log $1'
#
# Each scope (e.g. '.source.coffee' above) can only be declared once.
#
# This file uses CoffeeScript Object Notation (CSON).
# If you are unfamiliar with CSON, you can read more about it in the
# Atom Flight Manual:
# http://flight-manual.atom.io/using-atom/sections/basic-customization/#_cson

'.html':
  'UV-text':
    'prefix': 'utext'
    'body': '<mark class="u">$1</mark>$2'
  'S-text':
    'prefix': 'stext'
    'body': '<mark class="s">$1</mark>$2'
  'M-text':
    'prefix': 'mtext'
    'body': '<mark class="m">$1</mark>$2'
  'L-text':
    'prefix': 'ltext'
    'body': '<mark class="l">$1</mark>$2'
  'R-text':
    'prefix': 'rtext'
    'body': '<mark class="r">$1</mark>$2'
  'K-text':
    'prefix': 'ktext'
    'body': '<mark class="k">$1</mark>$2'
  'W-text':
    'prefix': 'wtext'
    'body': '<mark class="w">$1</mark>$2'
  'etal':
    'prefix': 'etal'
    'body': '<em>et al.,</em>'
  'Knock-out':
    'prefix': 'ko'
    'body': '_$1<sup>-/-$2</sup>_'
  'Morpholino':
    'prefix': 'mo'
    'body': '_$1<sup>MO</sup>_'
  'R-arrow':
    'prefix': 'rarr'
    'body': '&rarr;'
  'L-arrow':
    'prefix': 'larr'
    'body': '&larr;'
  'Up-arrow':
    'prefix': 'uarr'
    'body': '&uarr;'
  'Down-arrow':
    'prefix': 'darr'
    'body': '&darr;'
  'Underline':
    'prefix': 'under'
    'body': '<u>$1</u>$2'
  'sub':
    'prefix': 'subscript'
    'body': '<sub>$1</sub>$2'
  'sup':
    'prefix': 'superscript'
    'body': '<sup>$1</sup>$2'
  'uL':
    'prefix': 'uL'
    'body': '&mu;L'
  'Ion':
    'prefix': 'ion'
    'body': '$1<sup>$2</sup>$3'
  'Calcium':
    'prefix': 'calcium'
    'body': 'Ca<sup>2+</sup>'
  'Cyte':
    'prefix': 'cyte'
    'body': '&#x1D1CD;'
  'degC':
    'prefix': 'celsius'
    'body': '&deg;C'
  'F0':
    'prefix': 'F0'
    'body': 'F&empty;'

